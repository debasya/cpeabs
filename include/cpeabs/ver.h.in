#ifndef __CPEABS_VERSION_H__
#define __CPEABS_VERSION_H__ 1

#define CPEABS_VERSION "@cpeabs_VERSION@"

#define CPEABS_VERSION_MAJOR @cpeabs_VERSION_MAJOR @
#define CPEABS_VERSION_MINOR @cpeabs_VERSION_MINOR @
#define CPEABS_VERSION_PATCH @cpeabs_VERSION_PATCH @

/* This is the numeric version of the cpeabs version number, meant for easier
 * parsing and comparisons by programs.  The CPEABS_VERSION_NUM define
 * will always follow this syntax:
 *
 *      0xAABBCC
 *
 * Where AA, BB, and CC are each major version, minor version, patch in hex
 * using 8 bits each.
 *
 * It should be safe to compare two different versions with the greater number
 * being the newer version.
 */
#define CPEABS_VERSION_NUM @cpeabs_BITWISE_VERSION @

#endif
